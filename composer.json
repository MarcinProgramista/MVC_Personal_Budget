{
    "require": {
        "twig/twig": "^3.0",
        "mailgun/mailgun-php": "^3.0",
        "nyholm/psr7": "^1.8",
        "phpmailer/phpmailer": "^7.0",
        "vlucas/phpdotenv": "^5.6",
        "gemini-api-php/client": "^1.7",
        "symfony/http-client": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Core\\": "Core/",
            "App\\Controllers\\": "App/Controllers/",
            "App\\Models\\": "App/Models/",
            "App\\": "App/"
        }
    },
    "config": {
        "platform": {
            "php": "8.3.6"
        },
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
